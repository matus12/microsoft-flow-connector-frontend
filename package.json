    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "filesize": "^3.3.0",
    "fs-extra": "^0.30.0",
